<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.mine.MineFragment"
    tools:ignore="MissingDefaultResource">

    <LinearLayout
        android:visibility="gone"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:id="@+id/isnotconnect"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:layout_marginTop="@dimen/dp20"
            android:src="@mipmap/wifi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        </ImageView>
        <TextView
            android:layout_marginTop="@dimen/dp10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:text="@string/systemerror"></TextView>
        <TextView
            android:text="@string/refresh"
            android:id="@+id/refresh"
            android:textColor="@color/primary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp20"></TextView>
    </LinearLayout>

    <ScrollView
        android:id="@+id/isconnect"
        android:layout_width="match_parent"
        android:layout_height="650dp">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_height="250dp" android:background="@mipmap/minebg">
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp" android:src="@mipmap/nologin"></ImageView>
            <TextView
                android:id="@+id/account"
                android:hint="@string/app_name"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp10"
                android:textSize="@dimen/h7_5"
                android:text=""></TextView>
            <TextView
                android:id="@+id/cutcount"
                android:layout_width="match_parent"
                android:textColor="@color/primary"
                android:textSize="@dimen/f32"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:text="100"></TextView>
            <TextView
                android:layout_width="match_parent"
                android:textSize="@dimen/h9"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:text="@string/shengcishu"></TextView>
        </LinearLayout>

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center_vertical" android:padding="@dimen/dp10"
            android:layout_marginTop="@dimen/dp20"
            android:id="@+id/personaldata"
            >
            <ImageView
                android:layout_width="30dp"
                android:src="@mipmap/ziliao"
                android:layout_height="30dp"></ImageView>
            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textSize="@dimen/h7"
                android:text="@string/grzl"
                android:layout_marginLeft="10dp" />
            <ImageView
                android:layout_width="@dimen/dp10"
                android:src="@mipmap/forward"
                android:layout_height="wrap_content"></ImageView>
        </LinearLayout>


        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center_vertical" android:padding="@dimen/dp10"
            android:layout_marginTop="@dimen/dp20"
            android:id="@+id/myaccount"
            >
            <ImageView
                android:layout_width="30dp"
                android:src="@mipmap/zhanghu"
                android:layout_height="30dp"></ImageView>
            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textSize="@dimen/h7"
                android:text="@string/wodezhanghu"
                android:layout_marginLeft="10dp" />
            <ImageView
                android:layout_width="@dimen/dp10"
                android:src="@mipmap/forward"
                android:layout_height="wrap_content"></ImageView>
        </LinearLayout>




        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/canshusetting"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center_vertical" android:padding="@dimen/dp10"
            android:layout_marginTop="@dimen/dp20"
            >
            <ImageView
                android:layout_width="30dp"
                android:src="@mipmap/canshu"
                android:layout_height="30dp"></ImageView>
            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textSize="@dimen/h7"
                android:text="@string/setcanshu"
                android:layout_marginLeft="10dp" />
            <ImageView
                android:layout_width="@dimen/dp10"
                android:src="@mipmap/forward"
                android:layout_height="wrap_content"></ImageView>
        </LinearLayout>




        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/lang"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center_vertical" android:padding="@dimen/dp10"
            android:layout_marginTop="@dimen/dp20"
            >
            <ImageView
                android:layout_width="30dp"
                android:src="@mipmap/yuyan"
                android:layout_height="30dp"></ImageView>
            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textSize="@dimen/h7"
                android:text="@string/setyuyan"
                android:layout_marginLeft="10dp" />
            <ImageView
                android:layout_width="@dimen/dp10"
                android:src="@mipmap/forward"
                android:layout_height="wrap_content"></ImageView>
        </LinearLayout>


        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center_vertical" android:padding="@dimen/dp10"
            android:layout_marginTop="@dimen/dp20"
            android:visibility="gone"
            >
            <ImageView
                android:layout_width="30dp"
                android:src="@mipmap/yuyan"
                android:layout_height="30dp"></ImageView>
            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textSize="@dimen/h7"
                android:text="@string/setyuyan"
                android:layout_marginLeft="10dp" />
            <ImageView
                android:layout_width="@dimen/dp10"
                android:src="@mipmap/forward"
                android:layout_height="wrap_content"></ImageView>
        </LinearLayout>



        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center_vertical" android:padding="@dimen/dp10"
            android:layout_marginTop="@dimen/dp20"
            android:id="@+id/xieyi"
            >
            <ImageView
                android:layout_width="30dp"
                android:src="@mipmap/xieyi"
                android:layout_height="30dp"></ImageView>
            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textSize="@dimen/h7"
                android:text="@string/xieyi"
                android:layout_marginLeft="10dp" />
            <ImageView
                android:layout_width="@dimen/dp10"
                android:src="@mipmap/forward"
                android:layout_height="wrap_content"></ImageView>
        </LinearLayout>



        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center_vertical" android:padding="@dimen/dp10"
            android:layout_marginTop="@dimen/dp20"
            android:id="@+id/manual"
            >
            <ImageView
                android:layout_width="30dp"
                android:src="@mipmap/xieyi"
                android:layout_height="30dp"></ImageView>
            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textSize="@dimen/h7"
                android:text="@string/Manual"
                android:layout_marginLeft="10dp" />
            <ImageView
                android:layout_width="@dimen/dp10"
                android:src="@mipmap/forward"
                android:layout_height="wrap_content"></ImageView>
        </LinearLayout>

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center_vertical" android:padding="@dimen/dp10"
            android:layout_marginTop="@dimen/dp20"
            android:id="@+id/gybj"
            >
            <ImageView
                android:layout_width="30dp"
                android:src="@mipmap/benji"
                android:layout_height="30dp"></ImageView>
            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textSize="@dimen/h7"
                android:text="@string/gybj"
                android:layout_marginLeft="10dp" />
            <ImageView
                android:layout_width="@dimen/dp10"
                android:src="@mipmap/forward"
                android:layout_height="wrap_content"></ImageView>
        </LinearLayout>



        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/signout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center_vertical" android:padding="@dimen/dp10"
            android:layout_marginTop="@dimen/dp20"
            >
            <ImageView
                android:layout_width="30dp"
                android:src="@mipmap/signout"
                android:layout_height="30dp"></ImageView>
            <TextView
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:textSize="@dimen/h7"
                android:text="@string/tcdl"
                android:layout_marginLeft="10dp" />
            <ImageView
                android:layout_width="@dimen/dp10"
                android:src="@mipmap/forward"
                android:layout_height="wrap_content"></ImageView>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp20" android:orientation="horizontal" android:gravity="center">
            <TextView
                android:textColor="@color/grayft"
                android:text="v"
                android:id="@+id/appversion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"></TextView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"></LinearLayout>


    </LinearLayout>


    </ScrollView>
</FrameLayout>